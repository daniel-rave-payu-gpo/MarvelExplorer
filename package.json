{
  "name": "marvel-explorer-service",
  "version": "0.0.0",
  "engines": {
    "node": "20"
  },
  "volta": {
    "node": "20.13.1"
  },
  "scripts": {
    "loadenv": "./scripts/loadenv/loadenv.sh -f env.yaml",
    "lint": "npx eslint . --ext .ts",
    "generate-api-types": "openapi-typescript src/swagger/marvel-api.yaml -o src/types/generated-api.ts",
    "build": "./scripts/build.sh",
    "start": "nodemon --watch",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@prisma/client": "^5.7.1",
    "@sinclair/typebox": "^0.31.28",
    "@types/pg": "^8.10.9",
    "axios": "^1.7.2",
    "bottleneck": "^2.19.5",
    "dotenv": "^16.3.1",
    "env-schema": "^5.2.1",
    "env-types": "^1.4.8",
    "fastify": "^4.27.0",
    "fastify-plugin": "^4.5.1",
    "http-status-codes": "^2.3.0",
    "pg": "^8.11.3",
    "pino": "^8.21.0",
    "prisma": "^5.7.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "depcheck": "^1.4.7",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "leasot": "^13.3.0",
    "nodemon": "^3.0.2",
    "openapi-typescript": "^7.8.0",
    "pino-pretty": "^10.3.0",
    "prettier": "^3.1.1",
    "prom-client": "^15.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
